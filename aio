#!/bin/bash
ADD=$2
JAR=/etc/aio	#Pasta das defini√ß√µes
RECEITA_PASTA=/home/apps/receitas	#Pasta das receitas
BASIS=$RECEITA_PASTA/basis	#Pasta das bases para receitas
PRATO=$RECEITA_PASTA/out	#Saida do Produto
RAIZ="https://raw.githubusercontent.com"
APPS=/home/apps	#Pasta dos apps
COMBURENT="pkg2appimage"
BINS=/usr/bin
LOCAL=/home/`whoami`/.local
LOCALBINS=$LOCAL/bin
LOCALICONS=$LOCAL/share/icons/hicolor/256x256/apps
SYSICONS=/usr/share/icons/hicolor/256x256/apps
ELINKS=$LOCAL/share/applications
NEWS="andryeltj/aio/master/store/news.html"
OVEN=""
JARRA=""
PRODUTO=""
CAMINHO=`pwd`
RFIND="0"
ITEMW=""
AUTOINTEGRAR=0
VERSIONR=202102060226
ISLOCAL=0
troca=""
VERSION=""

case "$(uname -i)" in
  x86_64|amd64)
#    echo "x86-64 system architecture"
    SYSTEM_ARCH="x86_64";;
  i?86)
#    echo "x86 system architecture"
    SYSTEM_ARCH="i686";;
  aarch64|arm64)
#    echo "ARM system architecture"
    SYSTEM_ARCH="aarch64";;
  unknown|AuthenticAMD|GenuineIntel)
#         uname -i not answer on debian, then:
    case "$(uname -m)" in
      x86_64|amd64)
#        echo "x86-64 system architecture"
        SYSTEM_ARCH="x86_64";;
      i?86)
#        echo "x86 system architecture"
        SYSTEM_ARCH="i686";;
      aarch64|arm64)
#        echo "ARM system architecture"
        SYSTEM_ARCH="arm64";;
    esac ;;
  *)
    echo "Unsupported system architecture, testing if ARM"
    exit 1;;
esac

echo $1 $2 $3 $4 $5
### Fun√ß√µes
CALLLOCAL(){
if [[ "$2" == "local" || "$3" == "local" || "$4" == "local" || "$5" == "local" || "$2" == "--local" || "$3" == "--local" || "$4" == "--local" || "$5" == "--local" ]]; then
  echo "local"
  ISLOCAL=1
fi
}

ADDLIBRARY(){
 LISTITEM=`cat $JAR/aio.list | grep -w "$ADD:" | awk '{print $1}'`
 if [ "$LISTITEM" == "$ADD:" ]; then
    DELLIBRARY; echo "Atualizando "$LISTITEM" lista."
 else
	echo "$ADD: $PRODUTO" >> $JAR/aio.list; echo "Item "$LISTITEM" adicionado a lista."
 fi
}

DELLIBRARY(){
 LISTITEM=`cat $JAR/aio.list | grep -w "$ADD:" | awk '{print $1}'`
 if [ "$LISTITEM" == "$ADD:" ]; then
	sed -i '/'$ADD:'/d' $JAR/aio.list; echo "Item removido da lista"	#Remove da lista o nome do app existente
 else
    echo "O Item n√£o foi encontrado na lista."
 fi
}

CHECKAIOVERSION(){
AIOR=`cat /tmp/book | grep -w "aio:" | awk '{print $2}'`
echo "AIO: $AIOR"
if [ "$VERSIONR" -lt "$AIOR" ]; then
    troca=troca
    echo "Existe uma nova atualiza√ß√£o do AIO."
    MSGU003
    GETAIO
fi
}

CKFREELIB(){
	FreeLibINFO=`cat $RECEITA_PASTA/$ADD.yml | grep -w "freelib:" | awk '{print $2}'`
	if [ "$FreeLibINFO" == "" ]; then
	 troca=troca
	else
	 echo "FreeLib: $FreeLibINFO"
	 mkdir $RECEITA_PASTA/$OVEN
	 wget -q --show-progress -O $RECEITA_PASTA/$OVEN/excludedeblist $RAIZ/andryeltj/aio/master/xlists/$FreeLibINFO
#	 if [ -e "$RECEITA_PASTA/$OVEN/excludedeblist" ]; then sed 's|$FreeLibINFO|#$FreeLibINFO|g' $RECEITA_PASTA/$OVEN/excludedeblist; else echo "BlackList nao baixada para a receita"; fi
	fi
}

DOCOOK(){
rm -Rf $PRATO
CKFREELIB
if [ -e "$RECEITA_PASTA/$ADD.yml" ]; then
 cd $RECEITA_PASTA; MSGA001;
 echo "##*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*##"
 pkg2appimage "$RECEITA_PASTA/$ADD.yml"	#COZINHA
 echo "##*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*#*##"
 LAUNCHLIB
fi
}

DELBINS(){
	rm -RF $BINS/$ADD	#Remove o bin√°rio principal
	while IFS= read -r line
	do
	  ITEM=`cat $line | awk '{print $1}'`
	  echo $ITEM
	  if [[ "$ITEM" == "$ADD" ]]; then	#Cria uma lista dos parametros anteriormente adicionados
		echo `cat $line | awk '{print $2}'` >> /tmp/idata.aio 	
	  fi
	done < "$JAR/idata.aio"
	
	while IFS= read -r line
	do
	  IDATARM=`cat $line | awk {print $0}`
	  rm -Rf $ELINKS/$IDATARM	  #Remove os atalhos de integra√ß√£o
	done < "/tmp/idata.aio"
}

DEL(){
	echo "A remover: "$ADD
	EXISTAPP=`cat $JAR/aio.list | grep -w "$ADD:" | awk '{print $2}'`	#Detecta o nome do app antigo
	if [ -f "$APPS/$EXISTAPP" ]; then rm -Rf $APPS/$EXISTAPP; echo "App "$EXISTAPP" Removido"; MSGD001; else echo "O app "$EXISTAPP" n√£o foi encontrado para remo√ß√£o"; fi	#Remove o app; se existir
	DELLIBRARY
}

DELOVEN(){
	OVEN=`cat $RECEITA_PASTA/$ITEMW.yml | grep "app:" | awk '{print $2}'`
	MISTURA=$RECEITA_PASTA/$OVEN
	if [ "$MISTURA/$OVEN.AppDir" ]; then rm -Rf $MISTURA; echo 'Forno limpo'; else echo "Aconteceu algum erro, revise a receita"; fi
}

GETAIO(){
	rm -Rf $JAR/aio
	wget -q --show-progress $RAIZ/andryeltj/aio/master/aio -P $JAR
	chmod +x $JAR/aio
	AIOR=`cat $JAR/aio | grep -w "VERSIONR=" | awk '{print $2}'`
	echo "üóò AIO: "$AIOR" < Atualizado para esta vers√£o"
}

GETRECEITA(){
 RECIPE=`cat /tmp/book | grep -w "$ITEMW:" | awk '{print $2}'`
 if [ "$ITEMW" == "$RECIPE" ]; then
  RFIND="1" 
  SACOLA=`cat /tmp/book | grep -w "$ITEMW:" | awk '{print $4}'`;
  if [ -f "$RECEITA_PASTA/$RECIPE.yml" ]; then rm -Rf $RECEITA_PASTA/$RECIPE.yml; fi
  wget -q --show-progress $RAIZ/$SACOLA/$RECIPE.yml -P $RECEITA_PASTA/
  if [ -f "$RECEITA_PASTA/$RECIPE.yml" ]; then
   OVEN=`cat $RECEITA_PASTA/$RECIPE.yml | grep "app:" | awk '{print $2}'`; echo "FORNO: $OVEN";
   notify-send "Instalando: "$OVEN
  else echo "Receita n√£o baixada"; fi
 else
  echo "Receita n√£o encontrada"
 fi 
}

GETCOMBURENT(){
echo "isInstall: $isInstall"
  if [ $isInstall == "1" ]; then REPOsit=`cat $JAR/jar.aio`; wget -q --show-progress -c $REPOsit/book -P /tmp/; else GETNETBOOK; fi
  if [ -f "/tmp/book" ]; then
   JARRA=`cat /tmp/book | grep -w "$COMBURENT:" | awk '{print $4}'`;
   wget -q --show-progress $RAIZ/$JARRA/$COMBURENT -P /etc/aio/; chmod +x $JAR/$COMBURENT;
   if [ -f "$JAR/$COMBURENT" ]; then echo "üñ¥  <====  üåê"; else echo "üñ¥  X====  üåê"; fi
  else echo "BOOK nao baixado"; fi
  if [ $isInstall == "1" ]; then rm -Rf /tmp/book; fi
}

GETMIND(){
GETINTEGRATE=https://github.com/TheAssassin/AppImageLauncher/releases/download/v2.2.0
if [[ "$SYSTEM_ARCH" == "x86_64" || "$SYSTEM_ARCH" == "amd64" || "$SYSTEM_ARCH" == "i686" ]]; then
 if [[ "$SYSTEM_ARCH" == "x86_64" || "$SYSTEM_ARCH" == "amd64" ]]; then
  MIND="appimagelauncher-lite-2.2.0-travis995-0f91801-x86_64.AppImage"; fi
 if [ "$SYSTEM_ARCH" == "i686" ]; then
  MIND="appimagelauncher-lite-2.2.0-travis995-0f91801-i386.AppImage"; fi
 if [ -e "$JAR/$MIND" ]; then troca=troca; else
  wget -q --show-progress -c $GETINTEGRATE/$MIND -O $JAR/$MIND;
  chmod -R 777 $JAR/$MIND; chmod +x $JAR/$MIND;
  if [ -e "$JAR/$MIND" ]; then echo "üñ¥  <====  üß†"; $JAR/$MIND install; else echo "üñ¥  X====  üß†"; fi
 fi 
fi
if [[ "$SYSTEM_ARCH" == "arm64" || "$SYSTEM_ARCH" == "armhf" ]]; then
 sudo apt install libqt5core5a libqt5dbus5 libqt5gui5 libqt5widgets5 -y;
 if [ "$SYSTEM_ARCH" == "arm64" ]; then
 MIND="appimagelauncher_2.2.0-travis995.0f91801.bionic_arm64.deb"; fi
 if [ "$SYSTEM_ARCH" == "armhf" ]; then
 MIND="appimagelauncher_2.2.0-travis995.0f91801.bionic_armhf.deb"; fi
 if [ -e "$JAR/$MIND" ]; then troca=troca; else
  wget -q --show-progress -c $GETINTEGRATE/$MIND -O AppIL.deb
  dpkg -i AppIL.deb && sudo apt --fix-broken install; rm -Rf AppIL.deb;
  if [ -e "/usr/bin/AppImageLauncher" ]; then echo "üñ¥  <====  üß†"; appimagelauncher %f && killall AppImageLauncherSettings; else echo "üñ¥  X====  üß†"; fi
 fi
fi
}

GETNETBOOK(){
    if [ "`whoami`" == "root" ]; then
	 echo "Operando como Root, o livro sera baixado depois";
    else
	 if [ -f "$JAR/jar.aio" ]; then troca=troca; else JARCONTENT; fi
	 if [ -f "/tmp/book" ]; then rm -Rf /tmp/book; fi	#REMOVE O LIVRO ANTIGO
	 while IFS= read -r line
	 do
		wget -q --show-progress -c $line/book -P /tmp/	#BAIXA O NOVO LIVRO
	 done < $JAR/jar.aio
    fi
}

GETNEWS(){
	varNEWS=`curl -s --progress-bar $RAIZ/$NEWS`
}

GETONLY(){
	## Checa se a receita e de montagem ou somente download
	## Baixa o conteudo ja gerado em outros sites e o integra ao sistema
	GETOnly=`cat $RECEITA_PASTA/$ADD.yml | grep -w "getonly:" | awk '{print $2}'`
	if [ "$GETOnly" == "" ]; then
	 DOCOOK
	else
	 echo "GETOnly: YES"
	 OVEN=`cat $RECEITA_PASTA/$ADD.yml | grep "app:" | awk '{print $2}'`;
	 mkdir $RECEITA_PASTA/$OVEN; mkdir $PRATO;
	 wget -q --show-progress -P $PRATO/ $GETOnly
	 LAUNCHLIB
	fi
}

GETPAGEBASE(){
 URL1=$RAIZ/andryeltj/aio/master/store/main.css
 URL2=$RAIZ/andryeltj/aio/master/store/jquery.js
 URL3=$RAIZ/andryeltj/aio/master/store/main.js
 wget -q --show-progress -c -P /tmp/ $URL1 $URL2 $URL3
}

JARCONTENT(){
  echo "https://raw.githubusercontent.com/andryeltj/receitas/master" > $JAR/jar.aio
}

LAUNCHLIB(){
 OVEN=`cat $RECEITA_PASTA/$ITEMW.yml | grep "app:" | awk '{print $2}'`
 MISTURA=$RECEITA_PASTA/$OVEN 
 if [ -e "$MISTURA" ]; then troca=troca; else echo "Forno n√£o gerado"; fi
 cd $CAMINHO
 if [ -e "$PRATO" ]; then	#Testa pra ver se foi cozinhado
  PRODUTO=`ls $PRATO`	#Pega o nome do app criado
  APPNMLIST=`cat $JAR/aio.list | grep -w '$ITEMW: $PRODUTO'`;
  if [ "$APPNMLIST" == "$ADD: $PRODUTO" ]; then #Atualiza os dados da lista
   DELLIBRARY; ADDLIBRARY
  else ADDLIBRARY	#Coloca o nome do app novo na lista 
  fi
 else
  echo "A tigela n√£o saiu do forno"
 fi
}

LISTJAR(){
 echo "
 #######################################################
 #######################################################
	<Nome>				<Categorias>"
 rm -Rf /tmp/book2
while IFS= read -r line
do
   echo $line | awk '{print "  "$1"					"$3}' >> /tmp/book2
done < /tmp/book

sed -i '/aio:/d' /tmp/book2
cat /tmp/book2
 echo " #######################################################
 #######################################################"
}

MOVEAPP(){
### Parte dessa fun√ß√£o s√≥ deve ser chamada se o Produto j√° existir mas aquele usuario ainda n√£o o tem > somente lincar
PRODUTO=`ls $PRATO`
echo "Produto: $PRODUTO"
if [ -e "$PRATO" ]; then
 if [ -e "$PRATO/$PRODUTO" ]; then 
  cp -Rf $PRATO/$PRODUTO $APPS;	#Move o app para a pasta de apps
  chmod 777 $APPS/$PRODUTO;
  rm -Rf $PRATO
  echo $ITEMW" foi instalado.";
  notify-send -i gtk-yes -u normal "$ITEMW foi instalado."
  else echo "X - Produto n√£o existe, ou n√£o encontrado"; fi
else echo "O Prato n√£o foi criado"; fi
}

STOREIN="/tmp/aiostore"
JsReload='<script type="text/javascript"> setTimeout("location.reload();",3000); </script>'
CKWEBSERVER(){
	if [ -e "/var/www" ]; then
	 STOREIN="/var/www/html/aiostore"
	 if [ -e "$STOREIN" ]; then troca=troca; echo "a pasta existe no apache"; else echo "a pasta n√£o existe no apache"; echo $STOREIN; sudo mkdir $STOREIN; sudo chmod -R 777 $STOREIN; fi
	 LinkSTORE="localhost/aiostore"
	 MetSTORE="http://"
	else
	 STOREIN="/tmp/aiostore"
	 if [ -e "$STOREIN" ]; then troca=troca; echo "a pasta em tmp"; else echo "a loja vai estar em "$STOREIN; mkdir $STOREIN; fi
	 LinkSTORE=$STOREIN"/index.html"
	 MetSTORE="file://"
	fi
	echo $JsReload >> $STOREIN/index.html
}
MOUNTLISTS(){
	CBook=""
	sed -i '/pkg2appimage:/d' /tmp/book
	sed -i '/aio:/d' /tmp/book
	while IFS= read -r line
    do
       Par01=`echo $line | awk '{print $1}' | sed 's|\:||g'`
       CBook=$CBook`echo "<li><a>"$Par01"</a>"``echo $line | awk '{print "<a>"$2"</a>"}'``echo $line | awk '{print "<a>"$3"</a>"}'``echo $line | awk '{print "<a>"$4"</a>"}'``echo $line | awk '{print "<a>"$5"</a>"}'``echo $line | awk '{print "<a>"$6"</a>"}'``echo $line | awk '{print "<a>"$7"</a>"}'``echo $line | awk '{print "<a>"$8"</a>"}'``echo $line | awk '{print "<a>"$9"</a>"}'``echo $line | awk '{print "<a>"$10"</a>"}'``echo $line | awk '{print "<a>"$11"</a>"}'``echo $line | awk '{print "<a>"$12"</a></li>"}'`
    done < /tmp/book
	CList=""
	while IFS= read -r line
    do
       Par02=`echo $line | awk '{print $1}' | sed 's|\:||g'`
       CList=$CList`echo "<li><a>"$Par02"</a>"``echo $line | awk '{print "<a>"$2"</a></li>"}'`
    done < /etc/aio/aio.list
}

MOUNTPAGE(){
  imgloadstore="data:image/webp;base64,"
  GETNETBOOK
  GETPAGEBASE
  GETNEWS
# MainCSS='<link rel="stylesheet" type="text/css" href="'
# Css01=$MainCSS$RAIZ'/andryeltj/aio/master/store/main.css">'
 rm -Rf /tmp/store.html
 CssLk01=`cat /tmp/main.css`	#DevMode
 JsLk01=`cat /tmp/jquery.js`
 JsLk02=`cat /tmp/main.js`
 Css01="<style>"$CssLk01"</style>"	#DevMode
# Css01='<link rel="stylesheet" type="text/css" href="main.css"/>'
 JsBASIS='<script type="text/javascript">'$JsLk01'</script>'
 JsMAIN='<script type="text/javascript">'$JsLk02'</script>'	#DevMode
# JsMAIN='<script type="text/javascript" src="main.js"></script>'
 JsBase64='<!--- BASE64 --->
<script type="text/javascript">
"use strict";jQuery.base64=(function($){var _PADCHAR="=",_ALPHA="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/",_VERSION="1.0";function _getbyte64(s,i){var idx=_ALPHA.indexOf(s.charAt(i));if(idx===-1){throw"Cannot decode base64"}return idx}function _decode(s){var pads=0,i,b10,imax=s.length,x=[];s=String(s);if(imax===0){return s}if(imax%4!==0){throw"Cannot decode base64"}if(s.charAt(imax-1)===_PADCHAR){pads=1;if(s.charAt(imax-2)===_PADCHAR){pads=2}imax-=4}for(i=0;i<imax;i+=4){b10=(_getbyte64(s,i)<<18)|(_getbyte64(s,i+1)<<12)|(_getbyte64(s,i+2)<<6)|_getbyte64(s,i+3);x.push(String.fromCharCode(b10>>16,(b10>>8)&255,b10&255))}switch(pads){case 1:b10=(_getbyte64(s,i)<<18)|(_getbyte64(s,i+1)<<12)|(_getbyte64(s,i+2)<<6);x.push(String.fromCharCode(b10>>16,(b10>>8)&255));break;case 2:b10=(_getbyte64(s,i)<<18)|(_getbyte64(s,i+1)<<12);x.push(String.fromCharCode(b10>>16));break}return x.join("")}function _getbyte(s,i){var x=s.charCodeAt(i);if(x>255){throw"INVALID_CHARACTER_ERR: DOM Exception 5"}return x}function _encode(s){if(arguments.length!==1){throw"SyntaxError: exactly one argument required"}s=String(s);var i,b10,x=[],imax=s.length-s.length%3;if(s.length===0){return s}for(i=0;i<imax;i+=3){b10=(_getbyte(s,i)<<16)|(_getbyte(s,i+1)<<8)|_getbyte(s,i+2);x.push(_ALPHA.charAt(b10>>18));x.push(_ALPHA.charAt((b10>>12)&63));x.push(_ALPHA.charAt((b10>>6)&63));x.push(_ALPHA.charAt(b10&63))}switch(s.length-imax){case 1:b10=_getbyte(s,i)<<16;x.push(_ALPHA.charAt(b10>>18)+_ALPHA.charAt((b10>>12)&63)+_PADCHAR+_PADCHAR);break;case 2:b10=(_getbyte(s,i)<<16)|(_getbyte(s,i+1)<<8);x.push(_ALPHA.charAt(b10>>18)+_ALPHA.charAt((b10>>12)&63)+_ALPHA.charAt((b10>>6)&63)+_PADCHAR);break}return x.join("")}return{decode:_decode,encode:_encode,VERSION:_VERSION}}(jQuery));
</script>'
 favicon='<link rel="shortcut icon" href="favicon.svg" />'
 HTMLINFO="<head><title>Store</title><meta charset=UTF-8>"$favicon"<link href='https://fonts.googleapis.com/css2?family=Titillium+Web:ital@1&display=swap' rel='stylesheet'> $Css01 $JsBASIS $JsBase64 $JsMAIN </head>"
# CBook=`cat /tmp/book`
 MOUNTLISTS
# CList=`cat /etc/aio/aio.list`
 HtmlHAreas="<div class='book'>"$CBook"</div><div class='list'>"$CList"</div>"
 AppInfo="<div class='appinfo'><div class='appname'>Bem vindo</div><i>Clique abaixo para exibir as informa√ß√µes do App</i><div class='passing'>‚öôÔ∏è¬∞¬∞¬∞¬∞¬∞¬∞¬∞¬∞</div><div class='pdthumbs'><img src='https://raw.githubusercontent.com/PapirusDevelopmentTeam/papirus-icon-theme/master/Papirus/64x64/devices/camera-photo.svg'><a></a><a></a><a></a><a></a></div><a class='applogo'></a><a class='ckrecipe' alt='Exibe a receita do app instalado'>üìë</a><button class='seta'></button></div>"
 Newspaper="<div class='newspaper'>"$varNEWS"</div>"
 Mural="<div class='mural'><div class='imgview'><center><div class='exhib'></div><div class='rule'><a>üì∑</a><a>üì∑</a><a>üì∑</a><a>üì∑</a></div></center></div><div class='fullinfo'><p></p><a class='xview'>X</a></div></div>";
 CORPO=$HtmlHAreas$Newspaper$AppInfo"<article><center style='margin-top:18%'><img src='"$imgloadstore"'/></center></article>"$Mural
 echo "Abrindo a loja"
 MainBAR="<div class='mainbar'><a class='aiologo'>AIO</a><span class='searchbar' contenteditable='true'></span><a class='x'>X</a><div style='float:right'><a>A</a><a href='aio:store-up' class='updatestore'>üîÑ</a></div><i class='infoqtd'></i></div>"
 echo "$HTMLINFO<body><center></center><div class='container'>$CORPO</div>$MainBAR</body>" > $STOREIN/index.html
 yes | cp -Rf $JAR/aio-store.svg $STOREIN/favicon.svg
 rm -Rf /tmp/main.css /tmp/jquery.js /tmp/main.js
	}

NATIVEFIER(){
	#A fun√ß√£o deve ser iniciada antes de DOCOOK para baixar no disco a base que vai para a receita.	
	NATIVEFIERCK=`cat $RECEITA_PASTA/$ADD.yml | grep -w "nativefier:" | awk '{print $2}'`
	if [ "$NATIVEFIERCK" == "yes" ]; then
	  if [[ -e "$BASIS/NativefierBasis.tar.xz" ]]; then
	   troca=troca;	  else
	   BASISLINK=`cat $RECEITA_PASTA/$ADD.yml | grep -w "nativefierlink:" | awk '{print $2}'`
	   wget -q --show-progress --no-check-certificate '$BASISLINK' -O $BASIS/NativefierBasis.tar.xz
	  fi
	fi
}

RUNPAGE(){
  CKWEBSERVER
  MOUNTPAGE
  xdg-open $MetSTORE$LinkSTORE
  LinkSTORE=""
}

TESTLOCAL(){
echo "##################### $ISLOCAL ###################"
if [ "$ISLOCAL" == "1" ]; then
  echo "##################### $ISLOCAL ###################"
  echo "üìú Offline"
  MSGU004
  OVEN=`cat $RECEITA_PASTA/$ADD.yml | grep "app:" | awk '{print $2}'`
else
  echo "üìú ONLINE"
  GETNETBOOK
  GETRECEITA
fi
}

HELP(){
  echo "##################################
#Ooops... Algo aconteceu de errado
##################################
#
#As op√ß√µes corretas s√£o:
#
# add ou install	instala o app
# del ou remove		remove a instala√ß√£o de um app
# upd ou update		Atualiza up app espec√≠fico
#	store			Abre a loja
#	store -r		Recarrega a Loja
#	-i			√â o mesmo que 'add'
#	-d			√â o mesmo que 'del'
#	-u			√â o mesmo que 'upd'
#
#
#"
}

############# MENSAGENS DO SISTEMA
MSGA001(){
	notify-send -i "Instalando: "$ADD"."
	}
MSGA002(){
	echo $ITEMW" foi instalado."; notify-send -i gtk-yes -u normal "$ITEMW foi instalado."
	}
MSGAE001(){
	notify-send -i "gtk-dialog-warning" "N√£o encontrado" "O App n√£o foi encontrado nas fontes"
	}
MSGD001(){
	notify-send -i gtk-dialog-question -u normal $ADD" Removido." 
	}
MSGU001(){
	notify-send -i gtk-refresh "Atualizando o app: $1 $2 $3 $4"
	}
MSGU002(){
	notify-send -i gtk-yes -u normal "Atualizar" "Instala√ß√£o de $1 $2 $3 $4 completa"
	}
MSGU003(){
	notify-send -i gtk-refresh "AIO Atualizado"
	}
MSGU004(){
	notify-send -i gtk-yes "A instala√ß√£o est√° sendo realizada com uma receita local"
	}
MSGUE001(){
	notify-send -i gtk-dialog-question -u normal "Como?" "N√£o consegui encontrar o app para atualizar"
}

isInstall="0"
InstallDEB(){
	wget -q --show-progress -c $GETINTEGRATE -O AppIL.deb && dpkg -i AppIL.deb && sudo apt --fix-broken install; rm -Rf AppIL.deb;
	appimagelauncher %f && killall AppImageLauncherSettings;
}
INSTALLAIO(){
	isInstall="1"; echo ".............Instalando............";
#Cria as pastas	
	if [ -e "$JAR" ]; then troca=troca; else sudo mkdir $JAR; sudo chmod -R 777 $JAR; fi
	if [ -e "$APPS" ]; then troca=troca; else sudo mkdir $APPS; sudo chmod -R 777 $APPS; if [ -e "$APPS" ]; then  echo " [üì•‚úî] Pasta de Apps criada"; fi fi
	if [ -e "$RECEITA_PASTA" ]; then troca=troca; else mkdir $RECEITA_PASTA; sudo chmod -R 777 $RECEITA_PASTA; if [ -e "$RECEITA_PASTA" ]; then echo " [üìú‚úî] Pasta das receitas criada"; fi	fi #Cria a pasta aonde v√£o ficar as bases
	if [[ -e "$BASIS" ]]; then troca=troca; else mkdir $BASIS; sudo chmod -R 777 $BASIS; if [ -e "$BASIS" ]; then echo " [üìú‚úî] Pasta das bases criada"; fi fi 	#Cria a pasta aonde v√£o ficar as bases
#Pega/Cria os arquivos
	if [ -e "$JAR" ]; then yes | mv $CAMINHO/aio $JAR; echo "üñ¥  <====  üìú"; else echo "JARRA nao foi criada"; fi
	if [ -f "$JAR/jar.aio" ]; then troca=troca; else JARCONTENT; fi
	if [ -f "$JAR/$COMBURENT" ]; then troca=troca; else GETCOMBURENT; fi
	MAKEWidgets
    if [ -f "$JAR/system.aio" ]; then troca=troca; else echo "" > $JAR/system.aio; sudo chmod 777 $JAR/system.aio; fi
#Linka/Copia os binarios
	if [ -f "$JAR/aio" ]; then if [ -f "$BINS/aio" ]; then rm -Rf $BINS/aio; ln -s $JAR/aio $BINS/aio; else ln -s $JAR/aio $BINS/aio; echo "üìúüñ¥  <_‚úî_>  ‚öôÔ∏è	-	AIO Linkado"; fi fi
	if [ -f "$JAR/$COMBURENT" ]; then if [ -f "$BINS/$COMBURENT" ]; then rm -Rf $BINS/$COMBURENT; ln -s $JAR/$COMBURENT $BINS/$COMBURENT; else sudo ln -s $JAR/$COMBURENT $BINS/$COMBURENT; echo "üìú  <_‚úî_>  ‚öôÔ∏è	-	Pkg2AppImage Linkado"; fi else echo "PKG2APPIMAGE nao foi baixado"; fi
	if [ -f "$JAR/aio-store.svg" ]; then sudo cp -Rf $JAR/aio-store.svg $SYSICONS/; if [ -f "$SYSICONS/aio-store.svg" ]; then echo "Icone da loja integrado"; else echo "Nao foi possivel integar o Icone da Loja ao sistema"; fi fi
#Instala o suporte
	echo "###########################"
	#Arch an Fedora put here command for install the APT.
	sudo apt update; sudo apt install build-essential libcurl4 curl rpm2cpio imagemagick -y;
#Instala o Integrador
	echo "[AppImageLauncher]
ask_to_move=true
destination=/home/apps
enable_daemon=true

[appimagelauncherd]
%23%20additional_directories_to_watch=~/otherApplications:/even/more/applications
%23%20monitor_mounted_filesystems=false" > $CAMINHO/.config/appimagelauncher.cfg
MMCK=`cat /usr/share/applications/mimeinfo.cache | grep "x-scheme-handler/aio=aio.desktop;"`
    if [ "$MMCK" == "" ]; then echo "x-scheme-handler/aio=aio.desktop;" >> /usr/share/applications/mimeinfo.cache; fi
	notify-send -i $JAR/aio-store.svg -u normal "Abra a AIO Store em:
Menu > Internet > AIO Store";
}

MAKEWidgets(){
if [ -e "/usr/share/applications/aio.desktop" ]; then troca=troca; else 
echo "[Desktop Entry]
Type=Application
Name=AIO
Exec=aio %u
Icon=utilities-terminal
Terminal=true
MimeType=x-scheme-handler/aio;
Categories=System;
NoDisplay=true" > $JAR/aio.desktop
echo "Adicionando o atalho do AIO"
sudo cp -Rf $JAR/aio.desktop /usr/share/applications
fi
if [ -e "/usr/share/applications/aio_store.desktop" ]; then troca=troca; else 
echo "[Desktop Entry]
Type=Application
Name=AIO Store
Exec=aio store
Icon=/usr/share/icons/hicolor/256x256/apps/aio-store.svg
Categories=System;Settings;Network;" > $JAR/aio_store.desktop
echo "Adicionando o atalho da AIO Store"
sudo cp -Rf $JAR/aio_store.desktop /usr/share/applications
fi
if [ -f "/usr/share/icons/hicolor/256x256/apps/aio-store.svg" ]; then troca=troca; else
echo '
<svg width="256" height="256" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
 <defs>
  <stop stop-color="#ffffff" id="stop6071"/>
  <stop stop-color="#800080" id="stop6073"/>
  <stop stop-color="#ffffff" id="stop6075"/>
  <stop stop-color="#800080" id="stop6077"/>
 </defs>
 <metadata id="metadata3716">image/svg+xml</metadata>
 <g>
  <title>background</title>
  <rect fill="none" id="canvas_background" height="258" width="258" y="-1" x="-1"/>
 </g>
 <g>
  <title>Layer 1</title>
  <image xlink:href="data:image/png;base64," id="svg_1" height="378" width="248" y="-42" x="4"/>
 </g>
</svg>' > $JAR/aio-store.svg
echo "Adicionando icone da Loja"
fi
}

##Criando as pastas no Usuario
if [ "`whoami`" == "root" ]; then troca=troca; else
 if [ -e "$LOCAL" ]; then troca=troca; else mkdir $LOCAL; fi
 if [ -e "$ELINKS" ]; then troca=troca; else mkdir $ELINKS; fi
fi

##########################################
##
##				LAUNCHERS
##
##########################################

ISONLINE=`service NetworkManager status | grep "manager:" | awk '/D*G/{print $13}'`
if [ "$ISONLINE" == "CONNECTED_GLOBAL" ]; then 
#### COMANDOS E INTEGRA√á√îES
if [[ "$1" == "add" || "$1" == "install" || "$1" == "-i" ]]; then
 if [[ "`whoami`" == "root" ]]; then echo "Por favor tente instalar Apps sem ser o Administrador"; else
#  echo "`whoami`"
  ADD=$2
  ITEMW=$2
  TESTLOCAL
  if [ "$RFIND" == "1" ]; then
   NATIVEFIER
   GETONLY
   if [[ "$3" == "-mb" || "$4" == "-mb" ]]; then echo "A Bases ser√£o mantidas"; else DELOVEN; fi
   MOVEAPP
   CHECKAIOVERSION
  fi
 fi
 
elif [[ "$1" == "del" || "$1" == "remove" || "$1" == "-d" ]]; then
 DEL

elif [[ "$1" == "list" || "$1" == "-l" ]]; then
 GETNETBOOK
 LISTJAR
 CHECKAIOVERSION
 
elif [[ "$1" == "upd" || "$1" == "update" || "$1" == "-u" ]]; then
 ADD=$2
 ITEMW=$2
 CALLLOCAL
 echo "Atualizando: "$ADD
 DEL
 TESTLOCAL
 NATIVEFIER
 GETONLY
 if [[ "$3" == "-mb" || "$4" == "-mb" ]]; then echo "A Bases ser√£o mantidas"; else DELOVEN; fi
 MOVEAPP
 CHECKAIOVERSION

elif [[ "$1" == "--help" || "$2" == "--help" || "$3" == "--help" || "$4" == "--help" || "$1" == "help" || "$2" == "help" || "$3" == "help" || "$4" == "help" ]]; then
 HELP

elif [[ "$1" == "store" ]]; then
 if [[ "$3" == "-r" || "$4" == "-r" ]]; then
  CKWEBSERVER
  MOUNTPAGE
  CHECKAIOVERSION
 else
  RUNPAGE
  CHECKAIOVERSION
 fi

elif [[ "$1" == "aio:store-up" ]]; then
 CKWEBSERVER
 MOUNTPAGE
 CHECKAIOVERSION
 
elif [[ "$1" == "f-i" ]]; then
 if [ "`whoami`" == "root" ]; then INSTALLAIO; else echo "Vamos instalar... Execute como Root:
$ sudo ./aio"; fi
else
 if [ "$1" == '' ]; then troca=troca; else if ["$isInstall" == "1"]; then RUNPAGE; CHECKAIOVERSION; fi fi
fi
GETMIND
if [ -e "$JAR" ]; then troca=troca; else if [ "`whoami`" == "root" ]; then echo 'iniciando instalacao'; INSTALLAIO; else echo "Vamos instalar... Execute como Root:
$ sudo ./aio"; fi fi	#Aciona a instalacao

if [[ "$1" == "" ]]; then troca=troca; else
 STOREC=`echo $1 | awk '{print $1}'`
 STORER=`echo $1 | awk '{print $2}'`
 if [[ "$STOREC" == "aio:add" ]]; then
  ADD=$STORER
  $2=$STORER
  ITEMW=$STORER
  CKWEBSERVER
  TESTLOCAL
  if [ $RFIND == "1" ]; then
   NATIVEFIER
   GETONLY
   DELOVEN
   MOVEAPP
   MOUNTPAGE
   CHECKAIOVERSION
  fi
 fi
 if [[ "$STOREC" == "aio:del" ]]; then
  ADD=$STORER
  $2=$STORER
  CKWEBSERVER
  DEL
  notify-send $ADD" removido."
  MOUNTPAGE
  CHECKAIOVERSION
 fi
fi
else
	echo "::::::: Conecte-se a Internet antes de  Instalar :::::::"
	notify-send "Conecte-se a Internet"
fi
